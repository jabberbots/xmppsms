# ВСЕ ДАННЫЕ НУЖНО ИЗМЕНИТЬ

# Данный параметр задает номер телефона на который будут отсылаться смс
# Работает на данный момент только с одним номером
[mobile]
number = +71234567890

# Адрес (JID) аккаунта-администратора бота
# Работает на данный момент только с одним аккаунтом
[admin]
user = admin@admin.ru

# Параметры двух аккаунтов ботов, поддерживаются один, два аккаунта
# Будет добавлена поддержка еще нескольких аккаунтов
[account0]
login = bot0@bot.ru
resource = bot
password = pass

[account1]
login = bot1@bot.ru
resource = bot
password = pass
